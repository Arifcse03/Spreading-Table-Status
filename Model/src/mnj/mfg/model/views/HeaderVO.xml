  <SQLQuery>
    <![CDATA[SELECT HeaderEO.DIVISION_ID, 
       HeaderEO.HEADER_ID, 
       HeaderEO.TABLE_NO, 
       HeaderEO.SPREDING_DATE, 
       HeaderEO.SALESORDER_ID, 
       HeaderEO.ADDITIONAL_LAY_LENGTH, 
       HeaderEO.PLAN_NUMBER, 
       HeaderEO.CUSTOMER_ID, 
      -- HeaderEO.STYLE, 
       HeaderEO.BPO, 
       HeaderEO.REMARKS, 
       HeaderEO.LAST_UPDATE_DATE, 
       HeaderEO.LAST_UPDATED_BY, 
       HeaderEO.CREATION_DATE, 
       HeaderEO.CREATED_BY, 
       HeaderEO.LAST_UPDATE_LOGIN, 
       HeaderEO.ATTRIBUTE1, 
       HeaderEO.ATTRIBUTE2, 
       HeaderEO.ATTRIBUTE3, 
       HeaderEO.ATTRIBUTE4, 
       HeaderEO.ATTRIBUTE5, 
       HeaderEO.ATTRIBUTE6, 
       HeaderEO.ATTRIBUTE7, 
       HeaderEO.ATTRIBUTE8, 
       HeaderEO.ATTRIBUTE9, 
       HeaderEO.ATTRIBUTE10, 
       HeaderEO.ATTRIBUTE11, 
       HeaderEO.ATTRIBUTE12, 
       HeaderEO.ATTRIBUTE13, 
       HeaderEO.ATTRIBUTE14, 
       HeaderEO.ATTRIBUTE15, 
       HeaderEO.BUYER_ID, 
      -- HeaderEO.SEASON,
       (select ar.CUSTOMER_NAME
          from ar_customers ar
         where ar.CUSTOMER_ID = HeaderEO.BUYER_ID) Buyer_NAME,
        ( SELECT  LISTAGG (ll.ATTRIBUTE1, ',') WITHIN GROUP (ORDER BY ll.ATTRIBUTE1) FROM MNJ_MFG_SPREADING_T_L  ll where ll.HEADER_ID = HeaderEO.header_id group by ll.header_id) cut_number,
        ( SELECT  LISTAGG (ll.CUT_NUMBER, ',') WITHIN GROUP (ORDER BY ll.CUT_NUMBER) FROM MNJ_MFG_SPREADING_T_L  ll where ll.HEADER_ID = HeaderEO.header_id group by ll.header_id) ratioPlanNumber,
        ( SELECT  LISTAGG (ll.MARKER_ID, ',') WITHIN GROUP (ORDER BY ll.MARKER_ID) FROM MNJ_MFG_SPREADING_T_L  ll where ll.HEADER_ID = HeaderEO.header_id group by ll.header_id) Marker_id,
        HeaderEO.POC_ID,
        S.season,
         S.style
FROM MNJ_MFG_SPREADING_T_H HeaderEO ,XX_OM_POC_H_T S
WHERE HeaderEO.DIVISION_ID = :p_org_id
and HeaderEO.POC_ID=s.POC_ID order by  HeaderEO.HEADER_ID desc]]>
