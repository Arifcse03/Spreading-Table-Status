  <SQLQuery>
    <![CDATA[select ply.cut_no,plylin.cut_no cut_nonew ,
       plylin.no_of_ply ply,
       plylin.no_of_ply no_of_ply_n,
       h.ratioplan_number,
       h.style,
       h.season,
       l.marker_id,
       l.yds,
       l.inch,
       round(l.meter,3) meter,
       l.marker_plan_number
  from MNJ_MFG_RATIOPLAN           h,
       MNJ_MFG_MARHER_INFO_H       hhh,
       MNJ_MFG_MARHER_INFO_L       l,
       MNJ_MFG_RATIOPLAN_PLYLINE   ply,
       MNJ_MFG_RATIOPLAN_PLYCFLINE plylin
 where h.ratioplan_id = ply.ratioplan_id
   and hhh.header_id = l.header_id
   and ply.cut_no = l.marker_plan_number
   and plylin.plyline_id=ply.plyline_id    
  and h.ratioplan_number=:ratioplan_number
  and hhh.DIVISION_ID = :p_org_id
  and not exists (select *  from MNJ_MFG_SPREADING_T_L mmst where nvl(mmst.ATTRIBUTE1,'XXX') = plylin.cut_no)
  order by plylin.cut_no]]>
  </SQLQuery>
